#pragma kernel computeFronts

RWStructuredBuffer<float3> weatherGridBuffer;
RWStructuredBuffer<float3> weatherTamponBuffer;
 
[numthreads(64,1,1)]
 
void computeFronts(int3 threadID : SV_GroupThreadID, int3 groupID : SV_GroupID, int3 dispatchID : SV_DispatchThreadID)
{
	weatherGridBuffer[dispatchID.x].x = 5;
	weatherTamponBuffer[dispatchID.x].x = weatherGridBuffer[dispatchID.x].x * 2;
}